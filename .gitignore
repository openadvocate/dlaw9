# This file contains .gitignore rules that are often used with Drupal projects.
# Because .gitignore is specific to your site and its deployment processes, you
# may need to uncomment, add, or remove rules.

# Ignore directories generated by Composer
/vendor/
/web/core/
/web/libraries/
/web/modules/composer/
/web/profiles/composer/
/web/themes/composer/
node_modules

# Ignore sensitive info.
/web/sites/sites.local.php
/web/sites/*/settings.php
/web/sites/*/settings.local.php
/web/sites/*/services.local.yml

# Ignore site specific configurations.
/config/default/google_analytics_reports_api.settings.yml
/config/default/system.date.yml
/config/default/system.site.yml
/config/default/system.theme.global.yml
/config/default/block.block.twitterfeedblock.yml
/config/default/gtranslate.settings.yml
/config/default/livechat.settings.yml
/config/default/beacon.settings.yml


# Ignore user-generated content.
/web/sites/*/files
/web/sites/*/private

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Ignore database files.
*.sql*

# OS generated files
.DS_Store
Thumbs.db
._*

# Vim generated files
*.un~

# PhpStorm generated files
.idea

# SASS
.sass-cache
*.css.map
*.css.min.map
